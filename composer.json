{
  "name": "jeidison/native-query",
  "description": "Laravel package for save native query in external file.",
  "license": "MIT",
  "authors": [
    {
      "name": "Jeidison Farias",
      "email": "jeidison.farias@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-dom": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "Jeidison\\NativeQuery\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jeidison\\NativeQuery\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jeidison\\NativeQuery\\Providers\\NativeQueryServiceProvider"
      ],
      "aliases": {
        "NamedQuery": "Jeidison\\NativeQuery\\FacadeNativeQuery"
      }
    }
  }
}
